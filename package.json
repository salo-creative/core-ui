{
  "name": "@salo/core-ui",
  "version": "0.10.7",
  "description": "Core Salo React UI library",
  "main": "./index.js",
  "sideEffects": false,
  "scripts": {
    "start": "start-storybook -p 6001 -s ./public",
    "bundle-modules": "rm -rf dist/ && babel --copy-files src --out-dir dist",
    "bundle-watch": "babel --copy-files src --watch --out-dir dist",
    "bundle": "npm run bundle-modules",
    "demo": "npm run test:output -u && npm start",
    "lint": "eslint ./src",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "jest --coverage --maxWorkers=10",
    "release": "chmod +x ./ci/release.sh && ./ci/release.sh",
    "npm-publish": "npm-publish ./dist",
    "build-storybook": "build-storybook -s ./public -o .out",
    "deploy-storybook": "storybook-to-ghpages",
    "test:output": "jest -u --json --outputFile=.storybook/jest-test-results.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SaloCreative/core-ui.git"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test-setup.js",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/_story.js",
      "!src/**/*.story.js",
      "!src/global/**/*.js"
    ],
    "notify": true,
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "globals": {
      "webpackVars": {
        "ENV": "test"
      }
    },
    "verbose": true,
    "testURL": "http://localhost:6001/"
  },
  "keywords": [
    "React",
    "Styled",
    "Components",
    "Storybook",
    "Jest"
  ],
  "author": "Rich Comber",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SaloCreative/core-ui/issues"
  },
  "homepage": "https://github.com/SaloCreative/core-ui#readme",
  "devDependencies": {
    "@babel/runtime": "^7.4.2",
    "@jamesbliss/storybook-state": "^0.0.1",
    "@salo/icons": "^1.0.8",
    "@sambego/storybook-state": "^1.3.4",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-jest": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-notes": "^5.1.11",
    "@storybook/addon-options": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/cli": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/storybook-deployer": "^2.8.1",
    "babel-cli": "^6.26.0",
    "babel-core": "~6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "~7.1.2",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "check-single-proptype": "^1.0.1",
    "circular-dependency-plugin": "^4.4.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^1.1.6",
    "identity-obj-proxy": "^3.0.0",
    "is-url": "^1.2.2",
    "jest": "^23.4.2",
    "jest-dom": "^3.0.0",
    "jest-styled-components": "^6.3.1",
    "node-sass": "^4.9.2",
    "npm-cli-login": "^0.1.1",
    "prop-types": "^15.5.10",
    "raw-loader": "^0.5.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.10.2",
    "sass-loader": "^6.0.6",
