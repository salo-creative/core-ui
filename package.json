    "webpack": "^4.29.6"
  },
  "peerDependencies": {
    "@salo/icons": "^1.0.8",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.2.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.0.1",
    "@testing-library/react": "^9.3.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.12",
    "apollo-link-batch-http": "^1.2.12",
    "apollo-link-context": "^1.0.18",
    "apollo-upload-client": "^11.0.0",
    "color": "^3.1.0",
    "dompurify": "^1.0.10",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "hoist-non-react-statics": "^3.2.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "polished": "^3.0.3",
    "react-apollo": "^3.0.1",
    "react-dates": "^20.1.0",
    "react-dropzone": "^10.1.9",
    "react-helmet": "^5.2.0",
    "react-ink": "^6.4.0",
    "react-onclickoutside": "^6.8.0",
    "universal-cookie": "^3.1.0",
    "yup": "^0.27.0"
  }
}
